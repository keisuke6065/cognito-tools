{
  "name": "cognito-tools",
  "description": "cognito tools",
  "version": "0.0.8",
  "author": "keisuke6065",
  "bin": {
    "cognito-tools": "./bin/run"
  },
  "bugs": "https://github.com/keisuke6065/cognito-tools/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "JSONStream": "^1.3.5",
    "aws-sdk": "^2.361.0",
    "bottleneck": "^2.16.1",
    "cli-ux": "^5.1.0",
    "csv": "^5.1.1",
    "csv-parse": "^4.3.1",
    "mkdirp": "^0.5.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/csv-stringify": "^1.4.3",
    "@types/jsonstream": "^0.8.30",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "codecov": "^3.1.0",
    "eslint": "^5.5",
    "eslint-config-oclif": "^3.1",
    "eslint-plugin-node": "^8.0.0",
    "globby": "^8",
    "istanbul": "^0.4.5",
    "mocha": "^5",
    "nyc": "^14.1.1",
    "sinon": "^7.2.3",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3.5"
  },
  "engines": {
    "node": ">=10.15.1"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/keisuke6065/cognito-tools",
  "keywords": [
    "oclif",
    "aws",
    "cognito",
    "aws-cognito",
    "backup",
    "restore"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cognito-tools",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "keisuke6065/cognito-tools",
  "scripts": {
    "start": "./bin/run",
    "backup": "./bin/run backup",
    "restore": "./bin/run restore",
    "delete": "./bin/run delete",
    "registration": "./bin/run force-registration",
    "oclif": "oclif",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "lint": "eslint .",
    "version": "oclif-dev readme && git add README.md",
    "test:coverage": "rm -rf .nyc_output/* &&istanbul cover nyc ./node_modules/.bin/_mocha --forbid-only \"./test/**/*.test.ts\" && CODECOV_TOKEN='b07b92fd-1320-4006-8073-d3f3e4105138' codecov -f .nyc_output/*"
  },
  "types": "lib/index.d.ts"
}
